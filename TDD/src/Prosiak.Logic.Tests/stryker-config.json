{
  "stryker-config": {
    "concurrency": 4,
    "mutation-level": "Advanced",
    "language-version": "latest",
    "additional-timeout": 5000,
    "mutate": [
      "**/*",
      "!**/PotatoPriceProvider.cs"
    ],
    "project": "Prosiak.Logic.csproj",
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "verbosity": "info",
    "reporters": [
      //"Dots",
      "Progress",
      "Html"
    ],
    "since": {
      "enabled": false,
      "ignore-changes-in": [],
      "target": "master"
    },
    "baseline": {
      "enabled": false,
      "provider": "disk",
      "azure-fileshare-url": "",
      "fallback-version": "master"
    },
    "test-projects": [],
    "test-case-filter": "",
    "ignore-mutations": [],
    "ignore-methods": [ "Thread.Sleep*" ],
    "report-file-name": "mutation-report",
    "break-on-initial-test-failure": false
  }
}